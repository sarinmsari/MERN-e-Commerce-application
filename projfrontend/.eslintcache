=\"\">\r\n            <div className=\"form-group mt-3\">\r\n              <label htmlFor=\"email\" className=\"text-right\">\r\n                Email\r\n              </label>\r\n              <input\r\n                className=\"form-control\"\r\n                type=\"email\"\r\n                name=\"email\"\r\n                id=\"email\"\r\n                value={email}\r\n                onChange={handleChange(\"email\")}\r\n              />\r\n            </div>\r\n            <div className=\"form-group mt-3\">\r\n              <label htmlFor=\"Password\" className=\"text-right\">\r\n                Password\r\n              </label>\r\n              <input\r\n                className=\"form-control\"\r\n                type=\"password\"\r\n                name=\"password\"\r\n                id=\"password\"\r\n                value={password}\r\n                onChange={handleChange(\"password\")}\r\n              />\r\n            </div>\r\n            <button\r\n              className=\"btn btn-success btn-block form-control mt-3\"\r\n              onClick={onSubmit}\r\n            >\r\n              SignIn\r\n            </button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    );\r\n  };\r\n\r\n  return (\r\n    <Base title=\"Sign In page\" description=\"User can signin here\">\r\n      {loadingMessage()}\r\n      {errorMessage()}\r\n      {signInForm()}\r\n      {performRedirect()}\r\n      <p className=\"text-white text-center\">{JSON.stringify(values)}</p>\r\n    </Base>\r\n  );\r\n};\r\n\r\nexport default Signin;\r\n","D:\\Academic\\Learn Code Online\\MERN\\projfrontend\\src\\auth\\helper\\index.js",[],"D:\\Academic\\Learn Code Online\\MERN\\projfrontend\\src\\auth\\helper\\AdminRoutes.js",[],"D:\\Academic\\Learn Code Online\\MERN\\projfrontend\\src\\auth\\helper\\PrivateRoutes.js",[],"D:\\Academic\\Learn Code Online\\MERN\\projfrontend\\src\\user\\UserDashBoard.js",[],"D:\\Academic\\Learn Code Online\\MERN\\projfrontend\\src\\adm